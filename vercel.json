{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/css/:path*",
      "destination": "/static/css/:path*"
    },
    {
      "source": "/js/:path*",
      "destination": "/static/js/:path*"
    },
    {
      "source": "/config/:path*",
      "destination": "/static/config/:path*"
    }
  ]
}
